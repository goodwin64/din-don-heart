{
  "plugins": [
    "transform-class-properties",
    ["babel-plugin-styled-components", {
      "displayName": true
    }]
  ],
  "presets": ["react", "env"],
  "env": {
    "production": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "development": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "test": {
      "plugins": [
        ["transform-es2015-modules-commonjs"],
        ["transform-object-rest-spread"]
      ]
    }
  }
}